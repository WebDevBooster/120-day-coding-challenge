{
  "name": "SCROLL FREE!",
  "version": "0.6",
  "manifest_version": 2,
  "description": "SCROLL FREE! breaks you free from the scroll lock jail that many websites put you in.",
  "homepage_url": "http://alexbooster.com",
  "icons": {
    "19": "icons/icon19.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}